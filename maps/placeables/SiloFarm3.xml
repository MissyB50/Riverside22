                </inputs>
                <outputs>
                    <output fillType="FORAGE" amount="1" />
                </outputs>
            </production>
        </productions>

        <storage isExtension="false" fillLevelSyncThreshold="50">
            <capacity fillType="WHEAT"   capacity="1000000" />
            <capacity fillType="BARLEY"  capacity="1000000" />
            <capacity fillType="OAT"     capacity="1000000" />
            <capacity fillType="SORGHUM" capacity="1000000" />
            <capacity fillType="CANOLA" capacity="1000000" />
            <capacity fillType="MAIZE" capacity="1000000" />
            <capacity fillType="OLIVE" capacity="1000000" />
            <capacity fillType="GRAPE" capacity="1000000" />
            <capacity fillType="SUNFLOWER" capacity="1000000" />
            <capacity fillType="SOYBEAN" capacity="1000000" />
            <capacity fillType="MANURE" capacity="1000000" />
            <capacity fillType="POTATO" capacity="1000000" />
            <capacity fillType="SUGARBEET" capacity="1000000" />
            <capacity fillType="STRAW" capacity="1000000" />
            <capacity fillType="SILAGE" capacity="1000000" />
            <capacity fillType="MILK" capacity="1000000" />
            <capacity fillType="SUGARCANE" capacity="1000000" />
            <capacity fillType="LIQUIDMANURE" capacity="1000000"/>
            <capacity fillType="DRYGRASS_WINDROW" capacity="1000000"/>
            <capacity fillType="GRASS_WINDROW" capacity="1000000"/>
            <capacity fillType="DRYGRASS_WINDROW" capacity="1000000"/>
            <capacity fillType="SUGARBEET_CUT" capacity="1000000" />
            <capacity fillType="DIGESTATE" capacity="1000000" />
            <capacity fillType="DIESEL" capacity="1000000" />
            <capacity fillType="MINERAL_FEED" capacity="1000000" />
            <capacity fillType="FORAGE" capacity="1000000" />
        </storage>

        <playerTrigger node="playerTrigger" />

        <sellingStation node="sellingStation" appearsOnStats="true" supportsExtension="false">
            <unloadTrigger exactFillRootNode="exactFillRootNode" aiNode="aiUnloadingNode" fillTypes="WHEAT BARLEY OAT SORGHUM CANOLA SOYBEAN MAIZE SUNFLOWER OLIVE GRAPE POTATO SUGARBEET SUGARCANE STRAW MILK MANURE SILAGE LIQUIDMANURE DRYGRASS_WINDROW GRASS_WINDROW SUGARBEET_CUT DIGESTATE MINERAL_FEED DIESEL FORAGE_MIXING FORAGE" />
			<baleTrigger triggerNode="baleTrigger" deleteLitersPerSecond="5000" />
        </sellingStation>

        <loadingStation node="loadingTrigger" supportsExtension="true" storageRadius="50">
            <loadTrigger triggerNode="loadingTrigger" fillLitersPerSecond="2000" dischargeNode="dischargeNode" fillTypes="WHEAT BARLEY OAT SORGHUM CANOLA SOYBEAN MAIZE SUNFLOWER OLIVE GRAPE POTATO SUGARBEET SUGARCANE STRAW MILK MANURE SILAGE LIQUIDMANURE DRYGRASS_WINDROW GRASS_WINDROW SUGARBEET_CUT DIGESTATE MINERAL_FEED DIESEL FORAGE_MIXING FORAGE" aiNode="aiLoadingNode">
                <effectNode effectClass="PipeEffect" effectNode="pipeEffect"      materialType="pipe"           fadeTime="0.5" maxBending="0" controlPoint="4.8 0 0 0"/>
                <effectNode                          effectNode="pipeEffectSmoke" materialType="unloadingSmoke" fadeTime="0.5" />
            </loadTrigger>
        </loadingStation>

        <sounds>
            <idle template="grainMillIdle" linkNode="0>" volumeScale="0.2"/>
            <active template="grainMillWork" linkNode="0>" volumeScale="0.4"/>
        </sounds>
    </productionPoint>


    <i3dMappings>
        <i3dMapping id="mapPosition" node="0>0|3" />
        <i3dMapping id="storage" node="0>0|2" />
        <i3dMapping id="loadingTrigger" node="0>0|1|0" />
        <i3dMapping id="dischargeNode" node="0>0|1|1" />
        <i3dMapping id="pipeEffect" node="0>0|1|2|0" />
        <i3dMapping id="pipeEffectSmoke" node="0>0|1|2|1" />
        <i3dMapping id="aiLoadingNode" node="0>0|1|3" />
        <i3dMapping id="markerLoading" node="0>0|1|4" />
        <i3dMapping id="exactFillRootNode" node="0>0|0|0" />
        <i3dMapping id="baleTriggerNode" node="0>0|0|1" />
        <i3dMapping id="aiUnloadingNode" node="0>0|0|2" />
        <i3dMapping id="markerUnloading" node="0>0|0|3" />
        <i3dMapping id="infoTrigger" node="0>2" />
        <i3dMapping id="aiSpline" node="0>0|4" />
        <i3dMapping id="testArea1Start" node="0>5|0" />
        <i3dMapping id="testArea1End" node="0>5|0|0" />
        <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>4|0" />
        <i3dMapping id="tipOcclusionUpdateArea1End" node="0>4|0|0" />
        <i3dMapping id="teleportNode" node="0>3" />
        <i3dMapping id="playerTrigger" node="0>1|0" />
        <i3dMapping id="playerTriggerMarker" node="0>1|0|0" />
    </i3dMappings>
</placeable>