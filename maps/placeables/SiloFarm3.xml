                <inputs>
                    <input fillType="SOYBEAN" amount="1" />
                </inputs>
                <outputs>
                    <output fillType="SOYBEAN" amount="1" />
                </outputs>
            </production>
            <production id="Sugarcane" name="$l10n_fillType_sugarCane" cyclesPerHour="1000" costsPerActiveHour="0">
