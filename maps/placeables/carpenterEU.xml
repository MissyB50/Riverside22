<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="productionPoint" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_shopItem_carpenter</name>
        <functions>
            <function>$l10n_function_productionPoint</function>
        </functions>
        <image>$data/placeables/lizard/productionPoints/carpenterEU/store_carpenterEU.png</image>
        <price>60000</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>productionPoints</category>
        <brush>
            <type>placeable</type>
            <category>production</category>
            <tab>factories</tab>
        </brush>
        <vertexBufferMemoryUsage>838144</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>125184</indexBufferMemoryUsage>
        <textureMemoryUsage>9699328</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>$data/placeables/lizard/productionPoints/carpenterEU/carpenterEU.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <testAreas>
            <testArea startNode="testArea1Start" endNode="testArea1End" />
        </testAreas>

        <sounds>
            <place template="smallImp" />
        </sounds>
    </placement>

    <clearAreas>
        <clearArea startNode="clearAreaStart" widthNode="clearAreaWidth" heightNode="clearAreaHeight"/>
    </clearAreas>

    <leveling requireLeveling="true" maxSmoothDistance="10" maxSlope="75" maxEdgeAngle="30" >
        <levelAreas>
            <levelArea startNode="clearAreaStart" widthNode="clearAreaWidth" heightNode="clearAreaHeight" groundType="asphalt"/>
        </levelAreas>
    </leveling>

    <indoorAreas>
        <indoorArea startNode="indoorArea1Start" widthNode="indoorArea1Width" heightNode="indoorArea1Height" />
        <indoorArea startNode="indoorArea2Start" widthNode="indoorArea2Width" heightNode="indoorArea2Height" />
    </indoorAreas>

    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
    </ai>

    <infoTrigger triggerNode="infoTrigger"/>

    <dynamicallyLoadedParts>
        <dynamicallyLoadedPart filename="$data/placeables/lizard/productionPoints/carpenterEU/props.i3d" node="0" linkNode="propsLinkNode" />
    </dynamicallyLoadedParts>

    <lights>
        <group weatherRequiredFlags="NIGHT" />

        <sharedLight filename="$data/shared/assets/lights/lizard/buildings/industrialWallLight.xml" linkNode="linkedLight1" groupIndex="1"/>
        <sharedLight filename="$data/shared/assets/lights/lizard/buildings/industrialWallLight.xml" linkNode="linkedLight2" groupIndex="1"/>
        <sharedLight filename="$data/shared/assets/lights/lizard/buildings/industrialWallLight.xml" linkNode="linkedLight3" groupIndex="1"/>
        <sharedLight filename="$data/shared/assets/lights/lizard/buildings/industrialWallLight.xml" linkNode="linkedLight4" groupIndex="1"/>
        <sharedLight filename="$data/shared/assets/lights/terraLed/fluorescentTube50W.xml"          linkNode="linkedLight5" groupIndex="1"/>
        <sharedLight filename="$data/shared/assets/lights/terraLed/fluorescentTube50W.xml"          linkNode="linkedLight6" groupIndex="1"/>

        <lightShape node="interiorSelfIllum" groupIndex="1"/>
        <lightShape node="interiorSelfIllumLOD" groupIndex="1"/>

        <realLights>
            <low>
                <light node="realLightsLow" groupIndex="1"/>
            </low>
            <high>
                <light node="realLightsHigh" groupIndex="1"/>
            </high>
        </realLights>
    </lights>

    <productionPoint>
        <productions>
            <production id="furnitureWood" name="%s (%s)" params="$l10n_fillType_furniture|$l10n_fillType_wood" cyclesPerHour="9" costsPerActiveHour="3">
                <inputs>
                    <input fillType="WOOD" amount="4" />
                </inputs>
                <outputs>
                    <output fillType="FURNITURE" amount="5" />
                    <output fillType="WOODCHIPS" amount="0.15" />
                </outputs>
            </production>
            <production id="furnitureBoards" name="%s (%s)" params="$l10n_fillType_furniture|$l10n_fillType_boards" cyclesPerHour="10" costsPerActiveHour="3">
                <inputs>
                    <input fillType="BOARDS" amount="5" />
                </inputs>
                <outputs>
                    <output fillType="FURNITURE" amount="7" />
                    <output fillType="WOODCHIPS" amount="0.1" />
                </outputs>
            </production>
        </productions>

        <storage isExtension="false" fillLevelSyncThreshold="50">
            <capacity fillType="WOOD"      capacity="30000" />
            <capacity fillType="BOARDS"    capacity="18000" />
            <capacity fillType="FURNITURE" capacity="4000" />
            <capacity fillType="WOODCHIPS" capacity="5000" />
        </storage>

        <sellingStation node="sellingStation" supportsExtension="false">
            <unloadTrigger exactFillRootNode="unloadTrigger" fillTypes="BOARDS" />
            <unloadTrigger fillTypes="WOOD BOARDS" >
                <woodTrigger triggerNode="woodTrigger" activationTriggerNode="woodActivationTrigger" />
            </unloadTrigger>
        </sellingStation>

        <loadingStation node="loadingStation" supportsExtension="false">
            <loadTrigger fillTypes="FURNITURE WOODCHIPS" triggerNode="loadTrigger" fillLitersPerSecond="2000" dischargeNode="dischargeNode">
                <!-- <effectNode effectClass="PipeEffect" effectNode="0|1|0|0|2|0" materialType="pipe" materialTypeId="1" fadeTime="0.5" maxBending="0" controlPoint="3.25 0 0 0"/> -->
                <!-- <effectNode                          effectNode="0|1|0|0|2|1" materialType="smoke"     materialTypeId="1" fadeTime="0.5" /> -->
            </loadTrigger>
        </loadingStation>

        <palletSpawner node="palletSpawner">
            <spawnPlaces>
                <spawnPlace startNode="spawnSpace1start" />
            </spawnPlaces>
        </palletSpawner>

        <playerTrigger node="playerTrigger" />

        <sounds>
            <idle template="carpenterIdle" linkNode="0>" />
            <active template="carpenterWork" linkNode="0>" />
        </sounds>
    </productionPoint>

    <triggerMarkers>
        <triggerMarker node="warningStripes" />
        <triggerMarker node="playerTriggerMarker"         filename="$data/shared/assets/marker/markerIcons.xml" id="WRENCH"           adjustToGround="true" />
        <triggerMarker node="unloadTriggerMarker"         filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD"           adjustToGround="true" />
        <triggerMarker node="woodActivationTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="EXCLAMATION_MARK" adjustToGround="true" />
    </triggerMarkers>

    <hotspots>
        <hotspot type="PRODUCTION_POINT" linkNode="unloadTriggerMarker" teleportNode="unloadTriggerMarker" />
    </hotspots>

    <i3dMappings>
        <i3dMapping id="interiorSelfIllum" node="0>0|0|0|2|0" />
        <i3dMapping id="propsLinkNode" node="0>0|0|1" />
        <i3dMapping id="interiorSelfIllumLOD" node="0>0|1|0|0" />
        <i3dMapping id="clearAreaStart" node="0>1|0" />
        <i3dMapping id="clearAreaWidth" node="0>1|0|0" />
        <i3dMapping id="clearAreaHeight" node="0>1|0|1" />
        <i3dMapping id="indoorArea1Start" node="0>2|0" />
        <i3dMapping id="indoorArea1Width" node="0>2|0|0" />
        <i3dMapping id="indoorArea1Height" node="0>2|0|1" />
        <i3dMapping id="indoorArea2Start" node="0>2|1" />
        <i3dMapping id="indoorArea2Width" node="0>2|1|0" />
        <i3dMapping id="indoorArea2Height" node="0>2|1|1" />
        <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>3|0" />
        <i3dMapping id="tipOcclusionUpdateArea1End" node="0>3|0|0" />
        <i3dMapping id="testArea1Start" node="0>4|0" />
        <i3dMapping id="testArea1End" node="0>4|0|0" />
        <i3dMapping id="infoTrigger" node="0>6" />
        <i3dMapping id="loadingStation" node="0>7" />
        <i3dMapping id="loadTrigger" node="0>7|0" />
        <i3dMapping id="dischargeNode" node="0>7|1" />
        <i3dMapping id="loadingStationTriggerMarker" node="0>7|2" />
        <i3dMapping id="sellingStation" node="0>8" />
        <i3dMapping id="unloadTrigger" node="0>8|0" />
        <i3dMapping id="woodTrigger" node="0>8|1" />
        <i3dMapping id="unloadTriggerMarker" node="0>8|2" />
        <i3dMapping id="woodActivationTrigger" node="0>8|3" />
        <i3dMapping id="woodActivationTriggerMarker" node="0>8|4" />
        <i3dMapping id="playerTrigger" node="0>9|0" />
        <i3dMapping id="playerTriggerMarker" node="0>9|0|0" />
        <i3dMapping id="palletSpawner" node="0>10" />
        <i3dMapping id="spawnSpace1start" node="0>10|0" />
        <i3dMapping id="warningStripes" node="0>11" />
        <i3dMapping id="realLightsLow" node="0>12|0|0" />
        <i3dMapping id="realLightsHigh" node="0>12|0|1" />
        <i3dMapping id="linkedLight1" node="0>12|1|0" />
        <i3dMapping id="linkedLight2" node="0>12|1|1" />
        <i3dMapping id="linkedLight3" node="0>12|1|2" />
        <i3dMapping id="linkedLight4" node="0>12|1|3" />
        <i3dMapping id="linkedLight5" node="0>12|1|4" />
        <i3dMapping id="linkedLight6" node="0>12|1|5" />
    </i3dMappings>
</placeable>
