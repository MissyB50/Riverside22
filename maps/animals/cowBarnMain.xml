        <i3dMapping id="markerMilk" node="0>12|0" />
        <i3dMapping id="markerAnimals" node="0>12|1" />
        <i3dMapping id="markerLiquidManure" node="0>12|2" />
        <i3dMapping id="markerFood" node="0>12|3" />
        <i3dMapping id="markerWater" node="0>8|7" />
