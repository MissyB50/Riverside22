<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>$l10n_storeItem_donutPallet</name>
        <image>$data/store/store_empty.png</image>
        <category>pallets</category>
        <brand>LIZARD</brand>
        <showInStore>false</showInStore>
        <canBeSold>false</canBeSold>
        <financeCategory>OTHER</financeCategory>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>maps/pallets/donutPallet.i3d</filename>
        <size width="1.4" length="1" height="0.5" />
        <canBeReset>false</canBeReset>
        <components>
            <component centerOfMass="0 0.1 0" solverIterationCount="10" mass="30"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
        <input allowed="false"/>
    </base>



    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="DONUT" capacity="2000"/>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="50" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="true"/>
    </dischargeable>

    <pallet fillUnitIndex="1" node="pallet_vis">
        <content>
            <object node="donutBox1" tensionBeltNode="donutBox1Tension"/>
            <object node="donutBox2" tensionBeltNode="donutBox2Tension"/>
            <object node="donutBox3" tensionBeltNode="donutBox3Tension"/>
            <object node="donutBox4" tensionBeltNode="donutBox4Tension"/>
            <object node="donutBox5" tensionBeltNode="donutBox5Tension"/>
            <object node="donutBox6" tensionBeltNode="donutBox6Tension"/>
            <object node="donutBox7" tensionBeltNode="donutBox7Tension"/>
            <object node="donutBox8" tensionBeltNode="donutBox8Tension"/>
            <object node="donutBox9" tensionBeltNode="donutBox9Tension"/>
            <object node="donutBox10" tensionBeltNode="donutBox10Tension"/>
            <object node="donutBox11" tensionBeltNode="donutBox11Tension"/>
            <object node="donutBox12" tensionBeltNode="donutBox12Tension"/>
            <object node="donutBox13" tensionBeltNode="donutBox13Tension"/>
            <object node="donutBox14" tensionBeltNode="donutBox14Tension"/>
            <object node="donutBox15" tensionBeltNode="donutBox15Tension"/>
            <object node="donutBox16" tensionBeltNode="donutBox16Tension"/>
        </content>

        <sounds>
            <unload template="palletsFillPlastic" pitchScale="2.2" />
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="4" additionalMountDistance="0.45" jointTransY="0.11" jointLimitToRotY="true"/>

    <i3dMappings>
        <i3dMapping id="donutPallet" node="0>" />
        <i3dMapping id="pallet_vis" node="0>0" />
        <i3dMapping id="dynamicMountTrigger" node="0>0|0" />
        <i3dMapping id="dischargeNode" node="0>0|1" />
        <i3dMapping id="donutBox1" node="0>0|2|0|0" />
        <i3dMapping id="donutBox1Tension" node="0>0|2|0|1" />
        <i3dMapping id="donutBox2" node="0>0|2|1|0" />
        <i3dMapping id="donutBox2Tension" node="0>0|2|1|1" />
        <i3dMapping id="donutBox3" node="0>0|2|2|0" />
        <i3dMapping id="donutBox3Tension" node="0>0|2|2|1" />
        <i3dMapping id="donutBox4" node="0>0|2|3|0" />
        <i3dMapping id="donutBox4Tension" node="0>0|2|3|1" />
        <i3dMapping id="donutBox5" node="0>0|2|4|0" />
        <i3dMapping id="donutBox5Tension" node="0>0|2|4|1" />
        <i3dMapping id="donutBox6" node="0>0|2|5|0" />
        <i3dMapping id="donutBox6Tension" node="0>0|2|5|1" />
        <i3dMapping id="donutBox7" node="0>0|2|6|0" />
        <i3dMapping id="donutBox7Tension" node="0>0|2|6|1" />
        <i3dMapping id="donutBox8" node="0>0|2|7|0" />
        <i3dMapping id="donutBox8Tension" node="0>0|2|7|1" />
        <i3dMapping id="donutBox9" node="0>0|2|8|0" />
        <i3dMapping id="donutBox9Tension" node="0>0|2|8|1" />
        <i3dMapping id="donutBox10" node="0>0|2|9|0" />
        <i3dMapping id="donutBox10Tension" node="0>0|2|9|1" />
        <i3dMapping id="donutBox11" node="0>0|2|10|0" />
        <i3dMapping id="donutBox11Tension" node="0>0|2|10|1" />
        <i3dMapping id="donutBox12" node="0>0|2|11|0" />
        <i3dMapping id="donutBox12Tension" node="0>0|2|11|1" />
        <i3dMapping id="donutBox13" node="0>0|2|12|0" />
        <i3dMapping id="donutBox13Tension" node="0>0|2|12|1" />
        <i3dMapping id="donutBox14" node="0>0|2|13|0" />
        <i3dMapping id="donutBox14Tension" node="0>0|2|13|1" />
        <i3dMapping id="donutBox15" node="0>0|2|14|0" />
        <i3dMapping id="donutBox15Tension" node="0>0|2|14|1" />
        <i3dMapping id="donutBox16" node="0>0|2|15|0" />
        <i3dMapping id="donutBox16Tension" node="0>0|2|15|1" />
    </i3dMappings>
</vehicle>
