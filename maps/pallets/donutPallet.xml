        <i3dMapping id="cakeBox14" node="0>0|2|13" />
        <i3dMapping id="cakeBox14Tension" node="0>0|2|13|4" />
        <i3dMapping id="cakeBox15" node="0>0|2|14" />
        <i3dMapping id="cakeBox15Tension" node="0>0|2|14|4" />
        <i3dMapping id="cakeBox16" node="0>0|2|15" />
        <i3dMapping id="cakeBox16Tension" node="0>0|2|15|4" />
    </i3dMappings>
</vehicle>
